{
  "name": "routomil",
  "version": "1.2.0",
  "description": "Routomil - Sync routes from Mapy.cz to Garmin Connect",
  "license": "MIT",
  "author": "HromekFr",
  "repository": {
    "type": "git",
    "url": "https://github.com/HromekFr/routomil.git"
  },
  "bugs": {
    "url": "https://github.com/HromekFr/routomil/issues"
  },
  "homepage": "https://github.com/HromekFr/routomil#readme",
  "keywords": [
    "chrome-extension",
    "garmin",
    "garmin-connect",
    "mapy-cz",
    "gpx",
    "route-sync",
    "cycling",
    "hiking"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run icons && webpack --mode production && npm run test:integration",
    "build:skip-tests": "npm run icons && webpack --mode production",
    "dev": "webpack --mode development --watch",
    "icons": "node scripts/generate-icons.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/extension-integration.test.js",
    "test:e2e": "jest tests/e2e --runInBand",
    "test:all": "npm run build:skip-tests && npm run test && npm run test:integration",
    "test:api": "node tests/integration/test-course-api.js cookies.txt export.gpx",
    "test:mapy": "node tests/integration/test-mapy-export-api.js",
    "security": "npm run security:db && npm run security:analyze",
    "security:db": "bash scripts/create-codeql-db.sh",
    "security:analyze": "bash scripts/run-codeql-analysis.sh",
    "security:quick": "bash scripts/run-codeql-analysis.sh --quick",
    "security:view": "bash scripts/view-codeql-results.sh",
    "security:verify": "bash scripts/verify-codeql-setup.sh",
    "security:clean": "rm -rf codeql-db codeql-results",
    "version": "node scripts/sync-version.js && git add manifest.json",
    "package": "node scripts/package-extension.js"
  },
  "dependencies": {
    "xmldom": "^0.6.0"
  },
  "optionalDependencies": {
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/xmldom": "^0.1.34",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "form-data": "^4.0.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^24.1.3",
    "mini-css-extract-plugin": "^2.9.0",
    "node-fetch": "^3.3.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
